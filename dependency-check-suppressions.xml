    <!-- Suppress test-only dependencies -->
    <suppress>
        <notes><![CDATA[
        Test dependencies are not included in production builds
        ]]></notes>
        <gav regex="true">^.*:(junit|mockito|testcontainers|h2).*:.*$</gav>
    </suppress>
    
    <!-- Suppress development tools -->
    <suppress>
        <notes><![CDATA[
        Development and build tools not included in runtime
        ]]></notes>
        <gav regex="true">^org\.projectlombok:lombok:.*$</gav>
    </suppress>
    
