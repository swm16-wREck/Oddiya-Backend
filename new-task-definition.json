{
  "containerDefinitions": [
    {
      "name": "oddiya",
      "image": "501544476367.dkr.ecr.ap-northeast-2.amazonaws.com/oddiya:revision-9",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "SERVER_PORT",
          "value": "8080"
        },
        {
          "name": "DATABASE_NAME",
          "value": "oddiya"
        },
        {
          "name": "DATABASE_HOST",
          "value": "172.31.10.25"
        },
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "aws,postgresql,no-redis"
        },
        {
          "name": "DATABASE_PORT",
          "value": "5432"
        },
        {
          "name": "JAVA_OPTS",
          "value": "-XX:+UseContainerSupport -XX:MaxRAMPercentage=75.0 -XX:InitialRAMPercentage=50.0 -Dspring.profiles.active=aws,postgresql,no-redis -Dspring.autoconfigure.exclude=org.springframework.boot.autoconfigure.data.redis.RedisAutoConfiguration,org.springframework.boot.autoconfigure.data.redis.RedisRepositoriesAutoConfiguration,org.springframework.cloud.aws.autoconfigure.s3.S3AutoConfiguration -Dapp.aws.s3.enabled=false"
        },
        {
          "name": "SPRING_REDIS_ENABLED",
          "value": "false"
        },
        {
          "name": "SUPABASE_URL",
          "value": "http://dummy.supabase.co"
        },
        {
          "name": "SPRING_CACHE_TYPE",
          "value": "none"
        },
        {
          "name": "SUPABASE_ANON_KEY",
          "value": "dummy-anon-key-postgresql"
        },
        {
          "name": "SUPABASE_SERVICE_KEY",
          "value": "dummy-service-key-postgresql"
        },
        {
          "name": "JPA_DDL_AUTO",
          "value": "update"
        },
        {
          "name": "DATABASE_USERNAME",
          "value": "oddiya_user"
        },
        {
          "name": "DATABASE_PASSWORD",
          "value": "OddiyaSecure2025!"
        },
        {
          "name": "MANAGEMENT_HEALTH_REDIS_ENABLED",
          "value": "false"
        },
        {
          "name": "SPRING_DATA_REDIS_ENABLED",
          "value": "false"
        },
        {
          "name": "AWS_S3_ENABLED",
          "value": "false"
        },
        {
          "name": "APP_AWS_S3_ENABLED",
          "value": "false"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/oddiya",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "systemControls": []
    }
  ],
  "family": "oddiya-task",
  "taskRoleArn": "arn:aws:iam::501544476367:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::501544476367:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024"
}
